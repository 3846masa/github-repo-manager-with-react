{
  "extends": ["tslint-react", "tslint-config-airbnb"],
  "rules": {
    "ter-arrow-parens": [true, "as-needed"],
    "import-name": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "jsx-no-string-ref": true,
    "jsx-self-close": true,
    "jsx-wrap-multiline": false
  }
}
